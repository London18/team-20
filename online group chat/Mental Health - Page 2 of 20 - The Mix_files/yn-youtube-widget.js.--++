angular.module('tmApp').controller('YtFeedController',['$scope','$http',function($scope,$http){$scope.videos={};$http.get('/wp-content/plugins/yn-youtube-widget/videos.json').then(function(response){$scope.videos=response.data.items;},function(response){});$scope.go=function(vId){window.top.location.href='https://www.youtube.com/watch?v='+vId;};}]).filter('duration',function(){return function(input){return input.replace('PT','').replace(/(\d)H/gi,"$1:").replace(/(\d)M/gi,"$1:").replace(/(\d{2})S/gi,"$1").replace(/(\d)S/gi,"0$1").replace(/\:$/g,'');};});