(function($){$.fn.openInNewWindow=function(){var w=550;var h=400;var positionLeft=window.screenLeft!==undefined?window.screenLeft:screen.left;var positionTop=window.screenTop!==undefined?window.screenTop:screen.top;var left=(($(window).width()/2)-(w/2))+positionLeft;var top=(($(window).height()/2)-(h/2))+positionTop;window.open($(this).attr('href'),'','width='+w+', height='+h+', top='+top+', left='+left);};}(jQuery));(function($){if($.browser.mobile){$('.shareOnWhatsapp').css("display","inline-block");$('.shareOnSMS').css("display","inline-block");}
if(navigator.userAgent.match(/(iPad|iPhone|iPod)/g)){$('.sharing_buttons a[href^="sms:"]').attr('href',function(){return $(this).attr('href').replace(/sms:\?/,'sms:&');});}
$('.new_window').click(function(e){e.preventDefault();$(this).openInNewWindow();});})(jQuery);